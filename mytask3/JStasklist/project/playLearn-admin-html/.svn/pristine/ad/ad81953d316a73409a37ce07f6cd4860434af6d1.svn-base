<div class="col-lg-12 col-md-12 col-sm-12 edging edging-top">
    <h5><strong> | </strong>{{title.name}}</h5>
    <table class="table table-striped gridiron edging">
        <tr>
            <th class="th-fixed">
                <label class="control-label label-fixed" for="tm_subject">科目名称</label>
            </th>
            <th>
                <input id="tm_subject" class="form-control">
            </th>
        </tr>
        <tr>
            <th class="th-fixed">
                <label class="control-label label-fixed">类型</label>
            </th>
            <th>
                <form>
                    <label class="control-label pull-left cursor" for="tm_type">
                        <input type="radio" name="type" id="tm_type" checked>自有
                    </label>
                    <label class="control-label cursor">
                        <input type="radio" name="type">校内
                    </label>
                    <label class="control-label pull-right cursor">
                        <input type="radio" name="type">合作
                    </label>
                </form>
            </th>
        </tr>
        <tr>
            <th>
                <button type="button" class="btn btn-primary" ui-sref="backStage.teachManage.subject">确定</button>
            </th>
            <th>
                <button type="button" class="btn btn-primary" ui-sref="backStage.teachManage.subject">取消</button>
            </th>
        </tr>
    </table>
</div>