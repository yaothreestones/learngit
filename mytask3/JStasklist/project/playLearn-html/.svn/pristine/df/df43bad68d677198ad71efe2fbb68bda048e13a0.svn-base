<div class="container-fluid communal-padding least communal-absolute">
    <div class="login-head">
        <a ui-sref="app"><img src="image/app/return.png" height="30" width="30"/></a><span>注册</span>
    </div>
    <div class="login-center">
        <div class="top-handset">
            <img src="image/app/cellphone.png" />
            <input  id="ds_name" type="text" class="push-button" value="15203435003"  placeholder="请输入手机号码"   />
        </div>
        <div class="top-code">
            <img src="image/app/code.png"/>
            <input  id="ds_name" type="text" class="push-button" placeholder="输入验证码"   required ng-model="user.testcode"/>
            <b></b>
            <span>
                <input type="button" value="短信验证" class="code-input" ng-click="getTestCode()" ng-class="codeColor" ng-value="description" ng-disabled="canClick">
            </span>
            <span>
                <input type="button" value="语音验证"  class="code-input"ng-click="getTestStart()" ng-class="codeColor" ng-value="speech" ng-disabled="startClick">
                 <!--<a style="margin-right: 2.63vw" ng-click="getTestStart()" ng-bind="speech" ng-disabled="startClick">语音验证</a>-->
            </span>
        </div>
        <div class="bottom-password">
            <img src="image/app/password.png"/>
            <input  id="ds_name" type="text"class="push-button"  placeholder="请输入登录密码" />
        </div>
    </div>
    <div class="press-button">
        <button type="button" class="btn btn-default">登录</button>
        <a class="pull-left" >立即注册</a>
        <a class="pull-right">忘记密码？</a>
    </div>
    <div class="clear"></div>
    <div class="hint">

        wwwwwwwwwwwwwwww
    </div>

    <div class="third-party ">
        <div class="third-title"><b></b><span>第三方账号登录</span><b></b></div>
        <div class="party-logo">
            <span>
                 <img src="image/app/Wechat.png" height="77" width="76"/>
                <p>微信登陆</p>
            </span>
            <span>
               <img src="image/app/microblog.png" height="77" width="76"/>
                <p>微博登陆</p>
           </span>
            <span><img src="image/app/qq.png" height="77" width="76"/>
            <p>QQ登陆</p>
            </span>
        </div>
    </div>
</div>
