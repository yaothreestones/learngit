<div class="col-lg-12 col-md-12 col-sm-12 edging edging-top">
    <h5><strong> | </strong>{{title.name}}</h5>
    <div>
        <label class="control-label label-fixed col-sm-2" for="tm_subject">科目名称</label>
        <input id="tm_subject" class="col-sm-10">
    </div>
    <label class="control-label label-fixed col-sm-3">类型</label>
    <label class="control-label cursor col-sm-3" for="tm_type">
        <input type="radio" name="type" id="tm_type" checked>自有
    </label>
    <label class="control-label cursor col-sm-3">
        <input type="radio" name="type">校内
    </label>
    <label class="control-label cursor col-sm-3">
        <input type="radio" name="type">合作
    </label>
    <div>
        <label class="col-sm-2">年级划分</label>
        <div class="col-sm-10">
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">一年级
                </label>
            </div>
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">二年级
                </label>
            </div>
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">三年级
                </label>
            </div>
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">四年级
                </label>
            </div>
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">五年级
                </label>
            </div>
            <div class="col-sm-2">
                <label class="cursor">
                    <input type="checkbox">六年级
                </label>
            </div>
        </div>
        <textarea class="col-sm-10 col-sm-offset-2" style="max-width: 83.334%"></textarea>
    </div>
    <div class="col-sm-4 col-sm-offset-2">
        <button type="button" class="btn btn-primary" ui-sref="backStage.teachManage.subject">确定</button>
    </div>
    <div class="col-sm-4">
        <button type="button" class="btn btn-primary" ui-sref="backStage.teachManage.subject">取消</button>
    </div>
</div>