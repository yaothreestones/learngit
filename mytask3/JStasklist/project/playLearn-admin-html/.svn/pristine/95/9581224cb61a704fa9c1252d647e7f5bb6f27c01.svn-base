<div class="panel pael-default">
    <div class="panel-body" style="border: 1px solid #f5f5f5;border-bottom: none">
        <from class="form-horizontal">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-4 col-sm-6 form-group">
                        <label class="col-sm-3 control-label" >
                            標題
                        </label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control"  disabled="none">
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 form-group">
                        <label class="col-sm-3 control-label">
                            发送人群
                        </label>
                        <div class="col-sm-9 ">
                            <select class="form-control" ng-init="selectedName = names[0]"
                                    ng-model="selectedName" ng-options="x for x in class">
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6 form-group">
                        <label class="col-sm-3 control-label">
                            消息类型
                        </label>
                        <div class="col-sm-9 ">
                            <select class="form-control" ng-init="selectedName = names[0]"
                                    ng-model="selectedName" ng-options="x for x in state">
                            </select>
                        </div>
                    </div>
                    <div class="row pd-bottom-2">
                        <div class="col-md-4 pull-right text-right">
                            <div params="vm.searchParams" class="ng-isolate-scope">
                                <button type="button" class="btn btn-success" ng-click="search()">查询</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </from>
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-heading">
        <span class="user_management">I 咨詢管理</span>
        <span class="pull-right pointer" ui-sref="backStage.serviceManage.increased">新增+</span>
    </div>
    <div class="panel-body">
        <div class="panel panel-default">
            <table class="table table-striped">
                <thead>
                <th>序号</th>
                <th>標題</th>
                <th>封面</th>
                <th>分類</th>
                <th>摘要</th>
                <th>狀態</th>
                <th>操作</th>
                </thead>
                <tbody>
                <tr ng-repeat="self in [1,2,3,4,5,6]">
                    <td>{{$index+1}}</td>
                    <td>學習學習學習學習學習學習</td>
                    <td><img src="image/logo.png" height="116" width="115"/></td>
                    <td>1</td>
                    <td>學習學習學習學習學習學習學習學習學習</td>
                    <td>1</td>
                    <td>
                        <button style="border: none;background: none;color: #337Ab7;outline:none; " type="button"
                                class="btn btn-sm  btn-primary "
                                ng-clock>
                            <!--1草稿，2上线-->
                            <a ui-sref="backStage.serviceManage.sees">查看</a>
                        </button>
                        <button style="border: none;background: none;color: #337Ab7;outline:none; " type="button"
                                class="btn btn-sm  btn-primary "
                                ng-clock>
                            <a  ui-sref="backStage.serviceManage.check" ng-click="thaw()" style="cursor:pointer;">編輯</a>
                        </button>
                        <button style="border: none;background: none;color: #337Ab7;outline:none; " type="button"
                                class="btn btn-sm  btn-primary "
                                ng-clock>
                            <a ng-click="grounding()" style="cursor:pointer;">上架</a>
                        </button>
                        <button style="border: none;background: none;color: #337Ab7;outline:none; " type="button"
                                class="btn btn-sm  btn-primary "
                                ng-clock>
                            <a ng-click="delete()" style="cursor:pointer;">刪除</a>
                        </button>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="row" >
            <div class="its col-xs-11 pull-right">
                <uib-pagination total-items="80" ng-model="currentPage" max-size="maxSize"
                                first-text="第一页" previous-text="上一页" next-text="下一页" last-text="最后页"
                                boundary-links="true" boundary-link-numbers="true"></uib-pagination>
            </div>
        </div>
    </div>
</div>