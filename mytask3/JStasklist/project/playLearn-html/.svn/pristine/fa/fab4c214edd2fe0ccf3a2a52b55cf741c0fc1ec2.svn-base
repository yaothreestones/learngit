body{
    font-size: 10px!important;
    background: #f0f4f7;
}
/*公用去掉padding*/
.communal-padding{
    padding: 0;
}
/*position: fixed*/
.communal-absolute{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 999;

}
/*清除浮动*/
.clear{
    clear: both;
}
/*开始页begin*/
.start{
    position: absolute;
    top:0px;
    right: 0px;
    bottom: 0px;
    left: 0px;
    background: url("../../image/app/context.png") no-repeat ;
    background-size: cover;
    z-index: -999;
    min-height: 177.5vw;
}
.center-img{
    display: block;
    margin-left: auto;
    margin-right: auto;
}
.picture-top{
    margin-top: 38.43vw;
    height: 23.9vw;
}
.picture-center{
    margin-top: 7.5vw;
    height: 6vw;
    width: 25.4vw;
}
.picture-bottom{
    width: 34.8vw;
    height: 11.4vw;
    margin-top: 4.21vw;
}
.entry{
    position: absolute;
    bottom: 17.18vw;
    margin: auto;
    left: 0;
    right: 0;
    width: 96vw;
}

.entry input{
    height: 13.75vw;
    outline: none;
    display: block;
    border-radius: 5px;
    font-size: 5vw;
    border: 0.31vw solid #a2c4fd;
    font-weight: 500;
}
.ipt-top{
    color: #44bcf2;
    background: #fff;
}
.ipt-bottom{
    margin-top: 3.12vw;
    color: #fff;
    background: none;
    border-color: #a1c3fd;

}
