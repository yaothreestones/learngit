<div class="col-lg-12 col-md-12 col-sm-12 edging while">
    <form class="form-horizontal" role="form">
        <fieldset>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_type">所属科目</label>
                <div class="col-sm-4">
                    <select class="form-control" id="ds_type">
                        <option>语文</option>
                        <option>数学</option>
                        <option>外语</option>
                    </select>
                </div>
                <label class="col-sm-1 control-label" for="ds_datum">所属教材</label>
                <div class="col-sm-4">
                    <input class="form-control" id="ds_datum">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_subject">所属课程</label>
                <div class="col-sm-4">
                    <select class="form-control" id="ds_subject">
                        <option>语文</option>
                        <option>数学</option>
                        <option>外语</option>
                        <option selected>全部</option>
                    </select>
                </div>
                <label class="col-sm-1 control-label" for="ds_period">课时名称</label>
                <div class="col-sm-4">
                    <input class="form-control" id="ds_period">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_host">状态</label>
                <div class="col-sm-4">
                    <select class="form-control" id="ds_host">
                        <option>全部</option>
                        <option>上架</option>
                        <option>下架</option>
                    </select>
                </div>
            </div>
            <button type="button" class="btn btn-default pull-right">查询</button>
        </fieldset>
    </form>
</div>

