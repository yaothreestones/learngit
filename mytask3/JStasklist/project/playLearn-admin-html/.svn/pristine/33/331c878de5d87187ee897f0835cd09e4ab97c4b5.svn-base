<div class="col-lg-12 col-md-12 col-sm-12 edging while">
    <h5><strong> | </strong>{{title.name}}</h5>
    <div class="within">
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label class="col-sm-1 control-label top-form" for="ds_name">所属科目
                </label>
                <label class="col-sm-11 control-label top-form bottom-label" for="ds_name">数学
                </label>
            </div>
        </form>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 edging">
        <form class="form-horizontal" role="form">
            <fieldset class="form-bottom">
                <div class="form-group">
                    <div class="row">
                        <label class="col-sm-1 control-label top-form " for="ds_name">课程名称</label>
                        <input id="ds_name" type="text" class="col-sm-11 top-form top-form-nomargin" ng-disabled="show">
                    </div>
                    <div class="form-group">
                        <label class="col-sm-1 control-label top-form">年级划分</label>
                        <div class="col-sm-10">
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">一年级
                                </label>
                            </div>
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">二年级
                                </label>
                            </div>
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">三年级
                                </label>
                            </div>
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">四年级
                                </label>
                            </div>
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">五年级
                                </label>
                            </div>
                            <div class="col-sm-2 top-form">
                                <label class="cursor">
                                    <input type="checkbox" ng-disabled="show">六年级
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">是否收费</label>
                        <div class="col-sm-11 top-form top-form-nomargin">
                            <input type="checkbox" ng-disabled="show">锁定
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">首页封面</label>
                        <div class="col-sm-11 top-form top-form-nomargin">
                            <img-upload ajax-ads="" img-src="" btn-title="上传" ng-disabled="" img-src="params.img"></img-upload>
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">列表封面</label>
                        <div class="col-sm-11 top-form top-form-nomargin">
                            <img-upload ajax-ads="" img-src="" btn-title="上传" ng-disabled="" img-src="params.img"></img-upload>
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">课程简介</label>
                        <div class="col-sm-11 top-form top-form-nomargin">
                            <textarea class="col-sm-10" style="min-width:100%" ng-disabled="show" ng-disabled="show"></textarea>
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">相关课程</label>
                        <div class="col-sm-11 top-form top-form-nomargin">
                            <div class="col-sm-2">1号栏位(上)</div>
                            <div class="col-sm-10">
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-11 col-sm-offset-1 top-form top-form-nomargin">
                            <div class="col-sm-2">2号栏位(中)</div>
                            <div class="col-sm-10">
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-11 col-sm-offset-1 top-form top-form-nomargin">
                            <div class="col-sm-2">3号栏位(下)</div>
                            <div class="col-sm-10">
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <select class="form-control" ng-disabled="show">
                                        <option>语文</option>
                                        <option>数学</option>
                                        <option>外语</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <label class="col-sm-1 control-label top-form" for="ds_name">相关课程</label>
                        <select class=" col-sm-11 top-form-nomargin top-form" ng-disabled="show">
                            <option>语文</option>
                            <option>数学</option>
                            <option>外语</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <label class="col-sm-1 control-label top-form" for="ds_name"></label>
                    <div class="col-sm-11 fastener">
                        <button type="button" class="btn ascertain" ui-sref="backStage.teachManage.courses">确定</button>
                        <button type="button" class="btn cancel" ui-sref="backStage.teachManage.courses">取消</button>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
</div>