<div class="col-lg-12 col-md-12 col-sm-12 edging while">
    <form class="form-horizontal" role="form">
        <fieldset>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_subject">年级</label>
                <div class="col-sm-3">
                    <select class="form-control" id="ds_subject">
                        <option>全部</option>
                        <option>自有</option>
                        <option>校内</option>
                        <option>合作</option>
                    </select>
                </div>
                <label class="col-sm-1 control-label" for="ds_grade">所属教材</label>
                <div class="col-sm-3">
                    <select class="form-control" id="ds_grade">
                        <option>全部</option>
                        <option>自有</option>
                        <option>校内</option>
                        <option>合作</option>
                    </select>
                </div>
                <label class="col-sm-1 control-label" for="ds_courses">任务名称</label>
                <div class="col-sm-3">
                    <select class="form-control" id="ds_courses">
                        <option>全部</option>
                        <option>自有</option>
                        <option>校内</option>
                        <option>合作</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_period">版本</label>
                <div class="col-sm-3">
                    <select class="form-control" id="ds_period">
                        <option>全部</option>
                        <option>上架</option>
                        <option>下架</option>
                    </select>
                </div>
                <label class="col-sm-1 control-label" for="ds_mission_name">所属课时</label>
                <div class="col-sm-3">
                    <input class="form-control" id="ds_mission_name">
                </div>
            </div>
            <button type="button" class="btn btn-default pull-right">查询</button>
        </fieldset>
    </form>
</div>

