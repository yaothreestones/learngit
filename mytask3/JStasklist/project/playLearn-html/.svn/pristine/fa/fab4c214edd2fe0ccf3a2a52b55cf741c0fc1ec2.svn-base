/*登录页app*/
.least{
    min-height: 177.7vw;
}
.login-head{
    line-height: 12.9vw;
    background: #039fe6;
    padding-left: 2.63vw;
}
.login-head img{
    height: 4.16vw;
    width: 4.16vw;
    display: inline-block;
   vertical-align: middle;
}
.login-head span{
    vertical-align: middle;
    margin-left: 8.33vw;
    font-size: 5vw;
    color: #ffffff;
}
.login-center{
    margin-top: 2.91vw;
}
.login-center div{
    width: 100%;
    height: 15.27vw;
    background: #ffffff;
    display: flex;
    align-items: center;
}
.top-handset{
    border-bottom:0.1vw solid #e4e4e4;
}
.top-handset img{
    width: 3.33vw;
    height: 5.13vw;
    margin-left: 6.66vw;
}
.bottom-password img{
    width: 3.61vw;
    height: 4.86vw;
    margin-left: 6.66vw;
}

.login-center input{
    border: none;
    outline-style: none;
    border-left: 0.1vw solid #e4e4e4;
    margin-left: 6.11vw;
    padding-left: 4.72vw;
    font-size: 3.88vw;
    word-spacing: 1px;
    letter-spacing: 0.3vw;
}
.press-button{
    padding: 0 2.63vw;
    margin-top: 6.38vw;

}
.press-button button{
    width: 100%;
    height: 12.22vw;
    margin-left: auto;
    margin-right: auto;
    background: #e1e1e1;
    color: #999999;
    font-size: 4.44vw;
    margin-bottom: 3.75vw;
}
.press-button a{
    color: #c5c7c8;
    font-size: 3.33vw;
}
.third-party{
    bottom: 33.75vw;
    text-align: center;
    position: absolute;
    margin: auto;
    left: 0;
    right: 0;
    width: 100%;
}
.third-title{
    width: 65.27vw;
    margin-left: auto;
    margin-right: auto;
    display: inline-block;
    font-size: 2.63vw;
    color: #999999;
    margin-bottom: 9.02vw;
}
.third-title span{
    margin: 0 1.66vw;
    color: #999999;
}
.third-title b{
    display: inline-block;
    height: 0.5vw;
    width: 27%;
    background: #d9d9d9;
    vertical-align: 0.8vw;
}
.party-logo{
    width: 65.27vw;
    height: 17.5vw;
    margin: auto;
}
.party-logo span{
    display: inline-block;
    width: 32%;
}
.party-logo img{
    width: 10.55vw;
    height: 10.55vw;
}
.party-logo p{
    margin-top: 2.77vw;
    font-size: 2.77vw;
    color: #626364;
}
