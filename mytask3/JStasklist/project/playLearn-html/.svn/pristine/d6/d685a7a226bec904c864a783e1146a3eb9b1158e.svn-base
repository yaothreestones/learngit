<div class="container-fluid communal-padding">
    <!--顶部-->
    <div class="login-head" ng-show="$root.config.headNav.isShow" style="position: fixed; top:0px; left: 0px; width: 100%;z-index: 99; padding-left: 0"  >
        <div class="pull-left">
            <!--登录页-->
        <a ng-show="$root.config.headNav.backBtn.isShow" style="white-space:pre;margin-left: 2.63vw;" ng-click="cancel()"><img class="return" src="image/app/return.png" height="30" width="30" /></a><span>{{$root.config.headNav.title}}</span>
            <!--首页-->
            <!--<<span>{{$root.config.headNav.title}}</span>-->
        </div>
        <div class="pull-right">
            <a class=" the-right" ng-show="$root.config.headNav.Skip.isShow">跳转</a>
            <a class=" the-right" ng-show="$root.config.headNav.search.isShow"><img class="search" src="image/app/search.png" height="5vw" width="5vw"/></a>
        </div>
    </div>

    <!--中间-->
    <div ui-view></div>
    <!--底部-->
    <div class="base" ng-show="$root.config.footNav.isShow"  style="position: fixed; bottom:0px; left: 0px; width: 100%; height:15.37vw;z-index: 99">
        <span class="tab">
            <img src="image/app/home-page.png"/>
            首页
        </span>
        <span>
             <img src="image/app/course.png" />
             课程
        </span>
        <span>
            <img src="image/app/information.png"/>
             资讯
        </span>
        <span>
            <img src="image/app/mine.png"/>
             我的
        </span>
    </div>
</div>

