<div class="col-lg-12 col-md-12 col-sm-12 edging while">
    <form class="form-horizontal" role="form">
        <fieldset>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_type">类型</label>
                <div class="col-sm-4">
                    <select class="form-control" id="ds_type">
                        <option>全部</option>
                        <option>自有</option>
                        <option>校内</option>
                        <option>合作</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label" for="ds_subject">课程名称</label>
                <div class="col-sm-4">
                    <input class="form-control" id="ds_subject">
                </div>
                <label class="col-sm-1 control-label" for="ds_host">状态</label>
                <div class="col-sm-4">
                    <select class="form-control" id="ds_host">
                        <option>全部</option>
                        <option>上架</option>
                        <option>下架</option>
                    </select>
                </div>
            </div>
            <button type="button" class="btn btn-default pull-right">查询</button>
        </fieldset>
    </form>
</div>

